{
	"usesTypeScript": true,
	"usesStyledComponents": true,
	"testLibrary": "Testing Library",
	"component": {
		"default": {
			"path": "src/components",
			"withStyle": true,
			"withTest": true,
			"withStory": false,
			"withLazy": false,
			"customTemplates": {
				"component": "./templates/TemplateName.tsx",
				"style": "./templates/TemplateName.style.ts",
				"test": "./templates/TemplateName.test.tsx"
			}
		},
		"route": {
			"path": "src/routes",
			"withStyle": true,
			"withTest": true,
			"withStory": false,
			"withLazy": false,
			"customTemplates": {
				"component": "./templates/TemplateName.tsx",
				"style": "./templates/TemplateName.style.ts",
				"test": "./templates/TemplateName.test.tsx"
			}
		}
	}
}
